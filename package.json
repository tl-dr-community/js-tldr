{
  "name": "js-tldr",
  "version": "1.0.0-beta.10",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "fuse.js": "^3.4.5",
    "keycodes": "^1.0.0",
    "localforage": "^1.7.3",
    "localforage-driver-session-storage": "^1.0.4",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "node-sass": "^4.13.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-popper": "^1.3.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-use": "^13.8.0",
    "serve": "^11.2.0",
    "url-join": "^4.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "cypress:open": "./node_modules/.bin/cypress open --project tests/e2e",
    "cypress:run": "./node_modules/.bin/cypress run --project tests/e2e",
    "dev": "react-scripts start",
    "eject": "react-scripts eject",
    "start": "serve -s ./build",
    "test": "npm run test:u && npm run test:e2e",
    "test:u": "jest tests/unitary",
    "test:e2e": "npm run cypress:run",
    "cm": "git-cz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RusinovAnton/js-tldr.git"
  },
  "keywords": [
    "JS",
    "ES",
    "docs",
    "methods",
    "Array.prototype",
    "String.prototype",
    "Regex.prototype"
  ],
  "author": "Anton Rusinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RusinovAnton/js-tldr/issues"
  },
  "homepage": "http://js-tldr.info/",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-conventionalcommits": "^4.2.3",
    "cypress": "^3.7.0",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
