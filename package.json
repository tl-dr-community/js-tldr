{
  "name": "js-tldr",
  "version": "1.0.0-beta.26",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "classnames": "^2.2.6",
    "fuse.js": "^6.0.0",
    "keycodes": "^1.0.0",
    "localforage": "^1.7.3",
    "localforage-driver-session-storage": "^1.0.4",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-popper": "^1.3.7",
    "react-router-dom": "^5.2.0",
    "react-use": "^13.27.1",
    "serve": "^11.3.1",
    "typescript": "^3.9.3",
    "url-join": "^4.0.1"
  },
  "scripts": {
    "build:app": "react-scripts build",
    "build:search-index": "babel-node ./build-search-index.js",
    "build": "npm install && npm run build:app && npm run build:search-index",
    "cm": "git-cz",
    "cypress:open": "./node_modules/.bin/cypress open --project tests/e2e",
    "cypress:run": "./node_modules/.bin/cypress run --project tests/e2e",
    "dev": "react-scripts start",
    "eject": "react-scripts eject",
    "start": "serve -s ./build",
    "test:e2e": "npm run cypress:run",
    "test:u": "jest tests/unitary",
    "test": "npm run test:u && npm run test:e2e"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RusinovAnton/js-tldr.git"
  },
  "keywords": [
    "JS",
    "ES",
    "docs",
    "methods",
    "Array.prototype",
    "String.prototype",
    "Regex.prototype"
  ],
  "author": "Anton Rusinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RusinovAnton/js-tldr/issues"
  },
  "homepage": "https://js-tldr.info/",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/classnames": "^2.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "commitizen": "^4.1.2",
    "conventional-changelog-conventionalcommits": "^4.3.0",
    "cypress": "^4.7.0",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
